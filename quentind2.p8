--room management

originx=448
camx=(originx-64)
camy=0

function next_room()

if fget(mget(p1.x/8,p1.y/8),2) then
		camx+=128
		p1.x=camx+5
		p2.x=(camx+32*3)
	end
	if fget(mget(p2.x/8,p2.y/8),3) then
		camx-=128
		p1.x=camx+32
		p2.x=(camx+128-5)
	end
end
