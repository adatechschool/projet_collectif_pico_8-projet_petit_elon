--player update
function player_update(player)

	--physics
	player.dy+=gravity
	player.dx*=friction

	--controls	
	if btn(â¬…ï¸,player.player_key) then
		player.dx-=player.acc
		player.running=true		
	end
		
	if btn(âž¡ï¸,player.player_key) then
		player.dx+=player.acc
		player.running=true		
	end
		
	if btnp(â¬†ï¸,player.player_key)
		and player.landed then
			player.sp=player.sp_jump
			player.dy-=player.boost
			player.landed=false
	end

	if btnp(ðŸ…¾ï¸, player.player_key)
		and player.can_def==true then
			defend(player)
	end
	
	if btnp(âŽ,player.player_key)
	 	and player.can_atk==true then
			attack(player)
	end	
	
	--alignement
	players_alignement()

	--attack
		--end of attack / reset can_atk	
	if time()>player.attack_timer and player.attacking then
		player.attacking=false
		player.can_atk=true
		player.can_def=true
	end
		--end of def /reset can_def
	if time()>player.def_timer and player.defending then
		player.defending=false
		player.can_def=true
		player.can_atk=true
	end
		-- attack collision cases
	if player.attacking and check_attack(player) then
		if player.other_player.attacking then
			resolve_double_attack()
		end
	end

	--players collision
	

	--check collision up and down
	if player.dy>0 then
			player.falling=true
			player.landed=false
			player.jumping=false
			
			if collide_map(player,"down",0) then
				player.landed=true
				player.sp=player.sp_landed
				player.falling=false
				player.dy=0
				player.y-=(player.y+player.h)%8
			end
				
			elseif player.dy<0 then
				player.jumping=true
				if collide_map(player,"up",0) then
					player.dy=0
			end

	end
	--check collision left and right
	
	if player.dx<0 then
		if collide_map(player,"left",1) then
			player.dx=0
		end
	
	elseif player.dx>0 then
		if collide_map(player,"right",1) then
			player.dx=0
		end
	end

		player.x+=player.dx
		player.y+=player.dy
end

